{
  "scenario": "phone_sellout_demo",
  "config": {
    "diagnose": {
      "case": "smart_phone",
      "target_inventory_months": 2.0,
      "lifecycle_phase": "LATE",
      "brand_position": "PREMIUM",
      "sales_trend_lookback_months": 3,
      "overstock_ratio": 1.10,
      "sales_slump_slope": -1.0,
      "margin_buffer": 0.15,
      "seasonal_prebuild_lookback_months": 6,
      "seasonal_prebuild_inv_months": 1.0,
      "seasonal_prebuild_reco_rate": 0.10,
      "seasonal_prebuild_backlog_threshold": 0.0
    },

    "decide": {
      "deny_operators": [
        "production_adjust_csv"
      ]
    },

    "apply": {
      "enabled": true
    },

    "apply_price_adjust_timeseries": {
      "enabled": false,
      "months": [
        "2025-11"
      ],
      "rate": -0.10,
      "items": ["sell_price"]
    },


    "production_adjust_timeseries": {
      "enabled": false,
      "months": [],
      "rate": 0.0,
      "delta": 0.0
    },

    "demand_adjust": {
      "months": [],
      "rate": 0.0
    }
  }
